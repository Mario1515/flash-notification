{
    "name": "mario1515/flash-notification",
    "description": "A Laravel facade for multi flash messages (success, error, info, etc.)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Petkov",
            "email": "mariopetkovnfsg@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Mario\\FlashNotification\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mario\\FlashNotification\\FlashNotificationServiceProvider"
            ],
            "aliases": {
                "Notification": "Mario\\FlashNotification\\Facades\\Notification"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
